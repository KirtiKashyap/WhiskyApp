<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".dashboard.ui.PassFragment"
        android:id="@+id/constraint_profile"
        android:fitsSystemWindows="true"
        android:background="@drawable/loginbg">
    <include
        android:id="@+id/main_header"
        layout="@layout/main_header_new"

        />
    <TextView
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main_header"
        android:textColor="@color/orangetextcolor"
        android:id="@+id/nogustTextView"
        android:textSize="@dimen/_15sdp"
        android:layout_width="wrap_content"
        android:text="0"
        android:layout_height="wrap_content"
        tools:ignore="RtlCompat" />
    <TextView
        android:layout_marginTop="10dp"
        android:text="0"
        android:textSize="@dimen/_15sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nogustTextView"
        android:textColor="@color/orangetextcolor"
        android:id="@+id/regustTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="RtlCompat" />
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>